{
  "extends": ["tslint:recommended", "tslint-config-airbnb"],
  "rules": {
    "no-any": true,
    "prefer-array-literal": [
      true,
      {
        "allow-type-parameters": true
      }
    ],
    "variable-name": [
      true,
      "ban-keywords",
      "check-format",
      "allow-pascal-case",
      "allow-leading-underscore"
    ],
    "import-name": false,
    "interface-name": false,
    "object-literal-sort-keys": false,
    "ordered-imports": false
  }
}
